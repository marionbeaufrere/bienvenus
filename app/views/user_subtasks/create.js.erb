function renderSubtask(){
  const subtask = document.getElementById('subtask_<%= @subtask_id %>');
  subtask.innerHTML = '<%= j render "tasks/subtask_form", subtask: @subtask, task: @task %>';
}
renderSubtask();

function refreshInitializer(innerHTML) {
  const initialTasks = document.getElementById('task_<%= task_id %>');
  initialTasks.innerHTML = '<%= j render "tasks/initializer", task: @task %>';
}
refreshInitializer();
